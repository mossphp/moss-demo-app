{
	"name": "moss/demo-app",
	"version": "v0.8.5",
	"description": "The PHP micro-framework",
	"keywords": ["microframework"],
	"type": "framework",
	"license": "MIT",
	"authors": [
		{
			"name": "Michal Wachowski",
			"email": "wachowski.michal@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/potfur/moss"
		}
	],
	"require": {
		"php": ">=5.3.4",
		"moss/framework": ">=1.0",
		"moss/twig-bridge": ">=1"
